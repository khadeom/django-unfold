{% set priority = ["features", "bug fixes", "documentation", "chores"] %}
{%- for type_ in priority %}
{% set commits = release["elements"][type_] -%}
{% for commit in commits -%}
{% if type_ != "unknown" -%}
* {% if "!:" in commit.commit.summary.rstrip() %}**BREAKING** {% endif %}{{ commit.commit.message.rstrip() }} ([`{{ commit.commit.hexsha[:7] }}`]({{ commit.commit.hexsha | commit_hash_url }}))
{% endif %}
{% endfor %}
{% endfor %}
